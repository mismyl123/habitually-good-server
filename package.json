    "knex": "^0.21.12",
    "knex-migrate": "^1.7.4",
    "morgan": "^1.10.0",
    "pg": "^7.9.0",
    "uuid": "^8.3.1",
    "winston": "^3.3.3",
    "xss": "^1.0.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^8.2.1",
